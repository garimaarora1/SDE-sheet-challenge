class Solution(object):
    def generate(self, numRows):
        """
        :type numRows: int
        :rtype: List[List[int]]
        """
        p=[[1]*i for i in range(numRows+1)]

        for i in range(1,numRows+1):
            for j in range(1,len(p[i-1])):
                #if j is 0 or j is i:
                #   p[i][j]=1
                #else:   
                p[i][j]=p[i-1][j-1]+p[i-1][j]
        return p[1:]